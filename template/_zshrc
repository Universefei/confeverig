# This is the primer(main stream) .zshrc template
# all config file needed execute when starting a new terminal can be put here with a source(.) syntax

echo 'sourcing ~/.zshrc'

# sourcing zshrc template from oh-my-zsh
. ~/.zshrc.from.ohmyzsh

# sourcing zshrc template from myconfig/template/${directory}
. ~/.zsh_myconfig/zshrc_option.bash
